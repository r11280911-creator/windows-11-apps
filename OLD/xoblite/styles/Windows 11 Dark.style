style.name:		Windows 11 Dark
style.author:		niivu
style.date:		June 2021
style.credits:		default style #1 -> xoblite bb5 RC4
style.comments:		http://xoblite.net/

! --------------------------------------------------

toolbar.appearance: Flat Gradient Vertical
toolbar.color1: #202020
toolbar.color2: #202020
toolbar.font: Segoe UI/7/Normal
toolbar.alignment: Center
toolbar.textColor: #FFFFFF
toolbar.marginWidth: 0
toolbar.borderWidth: 1
toolbar.borderColor: #202020

toolbar.label.appearance: ParentRelative
toolbar.label.font: Blank/1/Normal
toolbar.label.alignment: left
toolbar.label.textColor: #FFFFFF
toolbar.label.marginWidth: 0

toolbar.windowLabel.appearance: Flat Gradient Vertical
toolbar.windowLabel.color1: #202020
toolbar.windowLabel.color2: #202020
toolbar.windowLabel.font: Segoe UI/7/Normal
toolbar.windowLabel.alignment: Center
toolbar.windowLabel.textColor: #FFFFFF
toolbar.windowLabel.marginWidth: 2
toolbar.windowLabel.borderWidth: 1
toolbar.windowLabel.borderColor: #202020

toolbar.clock.appearance: ParentRelative
toolbar.clock.font: Segoe UI/7/Normal
toolbar.clock.alignment: Center
toolbar.clock.textColor: #FFFFFF
toolbar.clock.marginWidth: 1

toolbar.button.appearance: ParentRelative
toolbar.button.picColor: #202020
toolbar.button.marginWidth: 0

toolbar.button.pressed.appearance: Flat Solid
toolbar.button.pressed.color: #414141
toolbar.button.pressed.picColor: #414141
toolbar.button.pressed.marginWidth: 0
toolbar.button.pressed.borderWidth: 0

! --------------------------------------------------

menu.title.appearance: Flat Solid
menu.title.color1: #202020
menu.title.font: Blank/1/Normal
menu.title.alignment: Left
menu.title.textColor: #202020
menu.title.shadowColor: #202020
menu.title.marginWidth: 1
menu.title.borderWidth: 1

menu.frame.appearance: Flat Gradient Vertical
menu.frame.color1: #202020
menu.frame.color2: #202020
menu.frame.font: Segoe UI/7/Normal
menu.frame.alignment: Left
menu.frame.textColor: #FFFFFF
menu.frame.disabledColor: #808080
menu.frame.bulletColor: #FFFFFF
menu.frame.marginWidth: 2
menu.frame.borderWidth: 1
menu.frame.borderColor: #808080

menu.active.appearance:  Flat Gradient Vertical
menu.active.color1: #414141
menu.active.color2: #414141
menu.active.textColor: #FFFFFF
menu.active.bulletColor: #FFFFFF
menu.active.marginWidth: 5
menu.active.borderWidth: 1
menu.active.borderColor: #414141

menu.grip.appearance: Flat Solid
menu.grip.color: #202020
menu.grip.font: Blank/1/Normal
menu.grip.alignment: Left
menu.grip.textColor: #202020
menu.grip.marginWidth: 0
menu.grip.borderWidth: 0

menu.indicator.appearance: Flat Solid
menu.indicator.color: #919191
menu.indicator.marginWidth: 2
menu.indicator.borderWidth: 2

menu.separator.appearance: Flat Solid
menu.separator.color: #202020

menu.bullet: Triangle
menu.bullet.position: Right

! --------------------------------------------------

dock.appearance: Flat Solid
dock.color: #FFFFFF
dock.marginWidth: 2
dock.borderWidth: 0

! --------------------------------------------------

console.color: #FFFFFF
console.textColor: #FFFFFF
console.buttonColor: #FFFFFF
console.font: Segoe UI/9/Normal

! --------------------------------------------------
